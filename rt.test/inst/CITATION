citHeader("To cite the weibullness package in publications, please use:")

citEntry(entry="Misc",
         author = personList(as.person("C. Park and M. Wang")),
         year   = "2018",
         title  = "Empirical distributions of the robustified t-test statistics",
         url = "https://arxiv.org/abs/1807.02215",
 textVersion = paste("Park, C. and M. Wang (2018).",
                    "Empirical distributions of the robustified t-test statistics.",
                    "ArXiv e-prints", "1807.02215.")
)
